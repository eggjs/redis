{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "extends": ["./node_modules/@eggjs/oxlint-config/.oxlintrc.json"],
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-console": "warn",
    "import/no-unassigned-import": "allow"
  },
  "ignorePatterns": ["index.d.ts", "test/fixtures/**", "__snapshots__"]
}
